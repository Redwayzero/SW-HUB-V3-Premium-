--Values

_G.autoStrength = true

_G.autoDurability = true 

_G.autoChakra = true

_G.autoSword = true

_G.autoSpeed = true 



--Function 

function autoStrength()
    while _G.autoStrength == true do
        local args = {
            [1] = "TrainStat",
            [2] = "Strength"
        }
        game:GetService("ReplicatedStorage").Events:FindFirstChild("Stats/RemoteFunction"):InvokeServer(unpack(args))
    end
end



function autoDurability()
    while _G.autoDurability == true do 
        local args = {
            [1] = "TrainStat",
            [2] = "Durability"
        }
        game:GetService("ReplicatedStorage").Events:FindFirstChild("Stats/RemoteFunction"):InvokeServer(unpack(args))
    end
end



function autoChakra()
    while _G.autoChakra == true do 
        local args = {
            [1] = "TrainStat",
            [2] = "Chakra"
        }
        game:GetService("ReplicatedStorage").Events:FindFirstChild("Stats/RemoteFunction"):InvokeServer(unpack(args))
    end 
end



function autoSword()
    while _G.autoSword == true do 
        local args = {
            [1] = "TrainStat",
            [2] = "Sword"
        }
        game:GetService("ReplicatedStorage").Events:FindFirstChild("Stats/RemoteFunction"):InvokeServer(unpack(args))
    end  
end



function autoSpeed()
    while _G.autoSpeed == true do
        local args = {
            [1] = "TrainStat",
            [2] = "Speed"
        }
         game:GetService("ReplicatedStorage").Events:FindFirstChild("Stats/RemoteFunction"):InvokeServer(unpack(args))
    end  
end


--Toggles 

StatsTab:AddToggle({
	Name = "Auto Strength",
	Default = false,
	Callback = function(Value)
		_G.autoStrength = Value
		autoStrength()
	end    
})
 
StatsTab:AddToggle({
	Name = "Auto Durability",
	Default = false,
	Callback = function(Value)
		_G.autoDurability = Value
		autoDurability()
	end    
